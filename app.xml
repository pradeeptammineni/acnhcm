<?xml version="1.0" encoding="UTF-8"?>
<Module>
    <ModulePrefs title="SAP-HCM App"
                author="Accenture"
           description="Accenture-Jive SAP-HCM App">
        <Require feature="dynamic-height"/>
        <Require feature="jive-core-v3"/>
        <Require feature="jive-connects-v1">
          <Param name="alias:SAPHCM|title:SAP HCM">jive:service://sap.com/api?version=1</Param>
        </Require>
        <Require feature="jquery-1.4.2"/>
        <Require feature="minimessage"/>
        <Require feature="osapi"/>
        <Require feature="views"/>
        <Link rel="Icon" href="https://github.com/downloads/pradeeptammineni/acnhcm/icon.png" /> 
        <Link rel="MediumIcon" href="https://github.com/downloads/pradeeptammineni/acnhcm/sap-logo.png" /> 
        <Link rel="LargeIcon" href="https://github.com/downloads/pradeeptammineni/acnhcm/sap-acn-icon.png" />
    </ModulePrefs>
    <Content view="home, canvas" type="html" href="hcmapp.html"/>
</Module>